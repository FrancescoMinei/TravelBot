<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <h3 class="text-center">
                    <div>
                        <h3>✈️Gestione dei messaggi predefiniti✈️</h3>
                        <form action="/message" method="POST">
                            <input name="Porta" type="text" value="<%= Porta %>"> <br>
                            <input name="WelcomeMsg" type="text" value="<%= welcomeMsg %>"> <br>
                            <input name="HelpMsg" type="text" value="<%= HelpMsg %>"><br>
                            <input name="View" type="text" value="<%= View %>"><br>
                            <input name="Position" type="text" value="<%= Position %>"><br>
                            <input name="Send" type="text" value="<%= Send %>"><br>
                            <input name="SendC" type="text" value="<%= SendC %>"><br>
                            <input name="Search" type="text" value="<%= Search %>"><br>
                            <input name="SearchC" type="text" value="<%= SearchC %>"><br>
                            <input name="SearchCity" type="text" value="<%= SearchCity %>"><br>
                            <input name="Searching" type="text" value="<%= Searching %>"><br>
